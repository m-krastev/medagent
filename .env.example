# Environment Variables for MedAgent
# Copy this file to .env and fill in your actual values

# Google AI Studio API Key (required)
# Get your key at: https://aistudio.google.com/app/apikey
GOOGLE_API_KEY=your_google_api_key_here

# Gemini Model Selection
# Fast model for triage, evidence, and imaging agents
MODEL_FAST=gemini-2.5-flash

# Reasoning model for hypothesis, research, and orchestrator agents
MODEL_REASONING=gemini-2.5-flash

# Embedding model for RAG (medical literature search)
MODEL_EMBEDDING=models/text-embedding-004
